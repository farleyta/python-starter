{
    "name": "Tim Farley Data",
    "build": {
        "dockerfile": "../Dockerfile",
        "context": "../"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-azuretools.vscode-docker",
                "ms-python.python",
                "ms-vscode-remote.remote-containers"
            ],
            "settings": {
                "editor.tabSize": 4,
                "terminal.integrated.defaultProfile.linux": "zsh"
            }
        }
    },
    "runArgs": [
        "--env-file",
        "src/.env"
    ]
}